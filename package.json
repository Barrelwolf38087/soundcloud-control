{
  "name": "soundcloud-control",
  "version": "0.3.0",
  "description": "SoundCloud control browser extension",
  "repository": {
    "type": "git",
    "url": "https://github.com/JohannesFischer/soundcloud-control.git"
  },
  "scripts": {
    "ff": "web-ext run -s src/ --bc",
    "lint:ext": "web-ext lint -s src/ -i background.js lib.js popup/popup.js",
    "build:ext": "web-ext build -s src/ -a build/ -i background.js lib.js popup/popup.js",
    "build:js": "webpack --config webpack.config.js",
    "build:js:watch": "./node_modules/.bin/webpack --config webpack.config.js --watch"
  },
  "keywords": [
    "soundcloud",
    "extension"
  ],
  "author": "Johannes Fischer",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "babel-loader": "^7.1.2",
    "babel-preset-import-export": "^1.0.2",
    "web-ext": "^2.2.2",
    "webpack": "^3.10.0"
  },
  "dependencies": {}
}
