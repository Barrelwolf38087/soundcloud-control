{
  "name": "soundcloud-control",
  "version": "0.1.0",
  "description": "SoundCloud control browser extension",
  "repository": {
    "type": "git",
    "url": "https://github.com/JohannesFischer/soundcloud-control.git"
  },
  "scripts": {
    "ff": "web-ext run -s src/",
    "lint:ext": "web-ext lint -s src/",
    "build:ext": "web-ext build -s src/ -a build/",
    "copy:polyfill": "shx cp ./node_modules/webextension-polyfill/dist/browser-polyfill.min.js ./src/browser-polyfill.js"
  },
  "keywords": [
    "soundcloud",
    "extension"
  ],
  "author": "Johannes Fischer",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "shx": "^0.2.2",
    "web-ext": "^2.2.2"
  },
  "dependencies": {
    "webextension-polyfill": "^0.2.1"
  }
}
