    "webextension",
    "firefox"
  ],
  "author": "Johannes Fischer",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "babel-loader": "^7.1.4",
    "babel-preset-import-export": "^1.0.2",
    "eslint": "^4.17.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "web-ext": "^2.4.0",
    "webpack": "^3.11.0"
  },
  "dependencies": {}
