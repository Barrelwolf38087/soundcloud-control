{
  "name": "soundcloud-control",
  "version": "0.5.3",
  "description": "SoundCloud Control browser extension",
  "repository": {
    "type": "git",
    "url": "https://github.com/JohannesFischer/soundcloud-control.git"
  },
  "scripts": {
    "ff": "web-ext run --bc",
    "lint:ext": "web-ext lint",
    "lint:js": "eslint ./src/",
    "build:ext": "web-ext build",
    "build:js": "webpack --config webpack.config.js",
    "build:js:watch": "webpack --config webpack.config.js --watch",
    "test": "npm run lint:ext && npm run lint:js"
  },
  "keywords": [
    "soundcloud",
    "webextension",
    "firefox"
  ],
  "author": "Johannes Fischer",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "babel-loader": "^7.1.4",
    "babel-preset-import-export": "^1.0.2",
    "eslint": "^4.19.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "web-ext": "^2.7.0",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8"
  },
  "dependencies": {
    "keycode": "^2.2.0",
    "regex-sort": "^1.0.6"
  }
}
